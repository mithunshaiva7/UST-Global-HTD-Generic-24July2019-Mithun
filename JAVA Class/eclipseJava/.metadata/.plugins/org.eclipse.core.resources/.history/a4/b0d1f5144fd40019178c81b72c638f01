package com.dev.threads;

public class Thread4 extends Thread{

	Printer p;
	WithoutSync wp;
	
	public Thread4(Printer pref) {
		p=pref;
	}
	
	public Thread4(WithoutSync prefW) {
		wp=prefW;
	}
	
	@Override
	public void run() {
		Thread.currentThread().sleep(500);
		//p.printVal(10, "Thread 4");
		wp.printVal(10, "Thread 4");
	}

}
