package com.dev.threads;

public class Thread4 extends Thread{

	Printer p;
	
	public Thread4(Printer pref) {
		p=pref;
	}
	
	@Override
	public void run() {
		try {
			Thread.currentThread().sleep(500);
		} catch (Exception e) {
			System.out.println("Excepion caught..");
		}
		p.printVal(4, "Thread 4");
	}

}
